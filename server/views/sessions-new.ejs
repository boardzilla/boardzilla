<%- include('partials/header') %>

<h1>New game</h1>

<form method="POST" action="/sessions">
  <select name="gameId">
    <% games.forEach(game => { %>
      <option value="<%=game.maxId%>"><%=game.name%></option>
    <% }) %>
  </select>
  <input type="submit" value="Create"></input>
</form>

<%- include('partials/footer') %>
